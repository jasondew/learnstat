<% require_bundle :dynarch_calendar %>
<h2> Create a Quiz </h2>

<%= error_messages_for :quiz %><br/>

<% form_tag do %>

<fieldset class="quiz_create">

  <label for="quiz_name">Name:</label><br/> <%= text_field :quiz, :name, :size => 30 %><br/>
  <label for="quiz_due">Due:</label><br/> <%= dynarch_datetime_select :quiz, :due %><br/>

  <%= submit_tag 'Create' %>

</fieldset>

<% end %>
