<h1>Editing grade</h1>

<%= error_messages_for :grade %>

<% form_for(:grade, :url => user_course_grade_path(current_user, @course, @grade), :html => { :method => :put }) do |f| %>
  <p>
    <%= submit_tag "Update" %>
  </p>
<% end %>

<%= link_to 'Show', user_course_grade_path(current_user, @course, @grade) %> |
<%= link_to 'Back', course_path(@course) %>
